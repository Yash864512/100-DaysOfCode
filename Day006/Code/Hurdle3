def turn_right():
    turn_left()
    turn_left()
    turn_left()

def goal():
    while front_is_clear():
        move()
        if at_goal():
            break
    if wall_in_front():
        jump()

def jump():
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()

while not at_goal():
    goal()
